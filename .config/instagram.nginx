server {
    # 80번 포트로 온 요청에 응답할 Block임
    listen 80;

    # HTTP요청의 Host값 (URL에 입력한 도메인)
    server_name localhost;

    # 인코딩 UTF-8설정
    charset urf-8;

    # root로부터의 요청에 대한 응답한 Block
    location / {
        # /run/gunicorn.sock파일을 사용해서 Gunicorn과 소캣통신하는 Proxy구성
        proxy_pass      http://unix:/run/instagram.sock;
    }
}
